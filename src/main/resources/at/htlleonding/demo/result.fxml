<VBox alignment="CENTER" prefHeight="355.0" prefWidth="500.0" spacing="70.0" xmlns="http://javafx.com/javafx/16"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.htlleonding.demo.controller.ResultController">
    <Label text="Finished!">
        <font>
            <Font size="40.0"/>
        </font>
    </Label>

    <Label fx:id="labelResult" textAlignment="CENTER"
           text="The game is over.&#10;The results of your games can be seen on the history screen.">
        <font>
            <Font size="25.0"/>
        </font>
    </Label>

    <Button fx:id="btnBack" text="Back to start" onAction="#onBackAction" scaleX="2" scaleY="2"/>
</VBox>
